<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Examen.Views.PagePrincipal">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Agregar Cont" x:Name="tooladd" Clicked="tooladd_Clicked" />
        <ToolbarItem Text="Lista Cont" x:Name="toollist" Clicked="toollist_Clicked" />
        <ToolbarItem Text="Mapa" x:Name="toolmap" Clicked="toolmap_Clicked" />
    </ContentPage.ToolbarItems>
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Examen de Programacion RAD" 
                   TextColor="Black"
                   TextDecorations="Underline"
                   
                   HorizontalOptions="CenterAndExpand">

            </Label>

            <Image Source="https://web.ucenm.net/wp-content/themes/edumy/images/logo.png"
                   VerticalOptions="CenterAndExpand">
                
            </Image>

            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>